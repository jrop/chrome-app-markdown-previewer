{
    "name" : "Markdown Previewer",
    "description" : "Displays a preview of markdown files.",
    "version" : "0.1",
    "app" : {
        "background" : {
            "scripts" : [ "js/background.js" ]
        }
    },
    "manifest_version" : 2,
    "minimum_chrome_version" : "25.0",
    "file_handlers" : {
        "text" : {
            "types" : [ "*/*" ],
            "title" : "Markdown Previewer"
        }
    },
    "permissions" : [ "fileSystem" ],
    "icons" : {
        "128" : "images/128.png"
    }
}
